@model IEnumerable<ECommerceProject.WebUI.Models.ViewComponent.NavigationViewModel>
<!-- NAVIGATION -->
<nav id="navigation">
    <!-- container -->
    <div class="container">
        <!-- responsive-nav -->
        <div id="responsive-nav">
            <!-- NAV -->
            <ul class="main-nav nav navbar-nav">
                @if (Model != null)
                {
                    @foreach (var nav in Model)
                    {
                        <li><a href="">@nav.Title</a></li>
                    }
                }



            </ul>
            <!-- /NAV -->
        </div>
        <!-- /responsive-nav -->
    </div>
    <!-- /container -->
</nav>
<!-- /NAVIGATION -->